#!/bin/bash

#Input string
input="Bash"

#Method 1: Using wc
count_wc=$(echo -n "$input" | wc -c)

#Method 2: Using loop
count_loop=0
for (( i=0; i<${#input}; i++ )); do
  ((count_loop++))
done

#Output
echo "Input string: $input"
echo "Character count using wc: $count_wc"
echo "Character count using loop: $count_loop"
